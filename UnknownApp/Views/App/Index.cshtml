@model IEnumerable<App>
@{
    <br />
    <br />
    ViewData["Title"] = "Unknown Apps";
}

<h1 class="text-center text-success">Unknown Apps</h1>
 
<div class="text-center">
    <div class="row">
        <div class="col-lg-16 text-end">
            
            <br /><br />
            
            <table class="table table-hover">
                <thead>
                    <tr>
                        
                        <th scope="col">AppName</th>
                        <th scope="col">Ratings</th>
                        <th scope="col">info</th>
                        <th scope="col">Link</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            
                            <td>@item.AppName</td>
                            <td>@item.Ratings</td>
                            <td>@item.info</td>
                            <td>@item.Link</td>
                            <td>
                                
                            </td>
                        </tr>
                    }
                </tbody>
            </table>

            
        </div>

    </div>
</div>
